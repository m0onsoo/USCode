class Solution:
    def kClosest(self, points: List[List[int]], k: int) -> List[List[int]]:
        min_heap = []
        for x, y in points:
            distance_squared = x**2 + y**2
            heapq.heappush(min_heap, (distance_squared, [x, y]))
        
        result = []
        for _ in range(k):
            distance, point = heapq.heappop(min_heap)
            result.append(point)
            
        return result