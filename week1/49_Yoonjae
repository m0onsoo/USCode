#NeetCode49: Group Anagram

from collections import defaultdict
from typing import List

class Solution:
    def groupAnagrams(self, strs: List[str]) -> List[List[str]]:
        sol = defaultdict(list)

        for s in strs:
            word = ''.join(sorted(s))
            sol[word].append(s)
        
        return list(sol.values())