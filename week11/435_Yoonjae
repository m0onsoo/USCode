class Solution:
    def eraseOverlapIntervals(self, intervals: List[List[int]]) -> int:
        if not intervals:
            return []
        intervals.sort(key=lambda x: x[1])
        merged = [intervals[0]]
        ct = 0
        for i in range(1, len(intervals)):
            if intervals[i][0] < merged[-1][1]:
                ct += 1
            else:
                merged.append(intervals[i])
        return ct