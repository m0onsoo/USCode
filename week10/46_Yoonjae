class Solution:
    def permute(self, nums: List[int]) -> List[List[int]]:
        if len(nums) == 0:
            return [[]]
        if len(nums) == 1:
            return [nums]
        sol = []
        for i in range(len(nums)):
            start = nums[i]
            rest = nums[:i] + nums[i+1:]
            for p in self.permute(rest):
                sol.append([start]+p)

        return sol

