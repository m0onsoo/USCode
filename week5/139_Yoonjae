class Solution:
    def wordBreak(self, s: str, wordDict: List[str]) -> bool:
        n = len(s)
        dp = [False] * (n+1)
        dp[0] = True

        for i in range(1, n+1):
            for word in wordDict:
                index = len(word)
                if s[i-index:i] == word and dp[i-index] == True and i >= 1:
                    dp[i] = True
        return dp[n]